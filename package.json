{
  "name": "dcwds",
  "private": true,
  "description": "The portfolio of Dalton Woods.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -H 0.0.0.0 -p 8000",
    "serve": "gatsby serve",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint-ts": "tslint -c tslint.json 'src/**/*/*.{ts,tsx}'",
    "type-check": "tslint-config-prettier-check ./tslint.json",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "{.ts,.tsx}": [
        "prettier --write",
        "tslint --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.3.34",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.0.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-switch": "^5.0.0",
    "styled-components": "^4.3.1",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "@types/prettier": "^1.16.4",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.16",
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.17.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/dcwoods/portfolio.git"
  }
}
